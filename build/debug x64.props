    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(OutDir);$(JSX)/extern/boost/lib/x64;$(JSX)/extern/openssl/lib/x64;$(JSX)/extern/v8/lib/debug/x64;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>glew64d.lib;ws2_32.lib;winmm.lib;libeay32.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>